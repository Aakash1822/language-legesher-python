'scopeName': 'text.python.console.legesher'
'name': 'Python Console Legesher'
'fileTypes': [
  'doctest'
  'pycon'
]
'patterns': [
  {
    'match': '^(>{3}|\\.{3}|In \\[\\d+\\]:) (.+)$'
    'captures':
      '1':
        'name': 'punctuation.separator.prompt.python.console.legesher'
      '2':
        'patterns': [
          'include': 'source.python.legesher'
        ]
  }
]
